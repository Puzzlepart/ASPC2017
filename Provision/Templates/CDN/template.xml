        <pnp:File Folder="{site}/SiteAssets/CDN/fonts" Overwrite="true" Src="..\..\dist\fonts\MATRIX.eot" />
        <pnp:File Folder="{site}/SiteAssets/CDN/fonts" Overwrite="true" Src="..\..\dist\fonts\MATRIX.woff" />
